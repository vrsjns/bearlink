datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ManagedLink {
  id          Int       @id @default(autoincrement())
  alias       String    @unique
  originalUrl String
  userId      Int
  clicks      Int       @default(0)
  expiresAt   DateTime?
  password    String?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}
